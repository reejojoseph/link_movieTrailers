<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
	<type-extension type-id="OrganizationPreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="genres">
                <display-name xml:lang="x-default">Supported Genres</display-name>
                <description xml:lang="x-default">eg: crime, action, comedy</description>
                <type>set-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="imdbAPIKey">
                <display-name xml:lang="x-default">IMDB API Key</display-name>
                <description xml:lang="x-default">IMDB API Key</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="maxResults">
                <display-name xml:lang="x-default">Maximum Results</display-name>
                <description xml:lang="x-default">Maximum number of results</description>
                <type>double</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="supportedLanguages">
                <display-name xml:lang="x-default">Supported Languages</display-name>
                <description xml:lang="x-default">&lt;languageID:language&gt; eg: fr:French</description>
                <type>set-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="youtubeAPIKey">
                <display-name xml:lang="x-default">Youtube API Key</display-name>
                <description xml:lang="x-default">Youtube API Key</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="MovieSearchSetting">
                <display-name xml:lang="x-default">Movie Search Setting</display-name>
                <attribute attribute-id="imdbAPIKey"/>
                <attribute attribute-id="youtubeAPIKey"/>
                <attribute attribute-id="maxResults"/>
                <attribute attribute-id="supportedLanguages"/>
                <attribute attribute-id="genres"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
